def slice
  fn |arr, start, finish|
    filter arr 
      fn |el, i|
        val result
          and
            i >= start true
            i < finish true
          end
        end

        result != false
      end
    end
  end
end

def length
  fn |arr|
    val result 0 end

    for arr
      fn |e, i|
        let result i end
      end
    end

    result
  end
end