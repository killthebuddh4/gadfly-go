std.write "Testing signals" end

signal
  sig_test

  (s)
    fn
      "feed_fail"
    end
end


def result
  emit
    "sig_test"

    on
      "feed_continue"

      (n)
        fn
          "continue"
        end
    end

    on
      "feed_fail"

      (n)
        fn
          "fail"
        end
    end
  end
end

std.write result end

