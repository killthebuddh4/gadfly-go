
signal
  SIG_TEST

  (sig)
    fn
      SIG_CONTINUE
    end
end

do
  emit
    SIG_TEST

    on
      SIG_CONTINUE

      (feedback)
        fn
          std.write COOL end
        end
    end
  end
end


def numbers
  array 1 2 3 4 5 end
end

def x
  fn
    throw
      SIG_FAIL
    end
  end
end

do
  catch
    SIG_FAIL

    (sig)
      fn
        std.write "Caught an error" end
      end
  end
  
  .x end
end
